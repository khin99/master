@model project1.Data.datamodel.model2

@{ Layout = null; }

<div class="row">
    <div class="col-md-12">
        <form asp-action="AddOrEdit" asp-route-id="@Model.ExampleKey" onsubmit="return jQueryAjaxPost(this);">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ExampleKey" />
            <div class="form-group">
                <label asp-for="Model1Key" class="control-label"></label>
                <input asp-for="Model1Key" class="form-control" />
                <span asp-validation-for="Model1Key" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sentence" class="control-label"></label>
                <input asp-for="Sentence" class="form-control" />
                <span asp-validation-for="Sentence" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InEnglish" class="control-label"></label>
                <input asp-for="InEnglish" class="form-control" />
                <span asp-validation-for="InEnglish" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InMyanmar" class="control-label"></label>
                <input asp-for="InMyanmar" class="form-control" />
                <span asp-validation-for="InMyanmar" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreatedDate" class="control-label"></label>
                <input asp-for="CreatedDate" class="form-control" />
                <span asp-validation-for="CreatedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UpdatedDate" class="control-label"></label>
                <input asp-for="UpdatedDate" class="form-control" />
                <span asp-validation-for="UpdatedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>